  <text x="35" y="280" class="heading">ID</text>
  <text x="80" y="280" class="heading">Nom</text>
  <text x="220" y="280" class="heading">Email</text>
  <text x="360" y="280" class="heading">Rôle</text>
  <text x="450" y="280" class="heading">Actions</text>
  
  <!-- Table Rows -->
  <rect x="20" y="290" width="560" height="40" class="odd-row"/>
  <text x="35" y="315" class="text">1</text>
  <text x="80" y="315" class="text">Alice Smith</text>
  <text x="220" y="315" class="text">alice@example.com</text>
  <text x="360" y="315" class="text">Admin</text>
  <rect x="450" y="300" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="460" y="315" class="button-text">Voir</text>
  <rect x="505" y="300" width="50" height="25" rx="4" fill="#5bc0de"/>
  <text x="515" y="315" class="button-text">Éditer</text>
  
  <rect x="20" y="330" width="560" height="40" class="even-row"/>
  <text x="35" y="355" class="text">2</text>
  <text x="80" y="355" class="text">Bob Johnson</text>
  <text x="220" y="355" class="text">bob@example.com</text>
  <text x="360" y="355" class="text">User</text>
  <rect x="450" y="340" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="460" y="355" class="button-text">Voir</text>
  <rect x="505" y="340" width="50" height="25" rx="4" fill="#5bc0de"/>
  <text x="515" y="355" class="button-text">Éditer</text>
  
  <rect x="20" y="370" width="560" height="40" class="odd-row"/>
  <text x="35" y="395" class="text">3</text>
  <text x="80" y="395" class="text">Charlie Brown</text>
  <text x="220" y="395" class="text">charlie@example.com</text>
  <text x="360" y="395" class="text">User</text>
  <rect x="450" y="380" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="460" y="395" class="button-text">Voir</text>
  <rect x="505" y="380" width="50" height="25" rx="4" fill="#5bc0de"/>
  <text x="515" y="395" class="button-text">Éditer</text>
  
  <rect x="20" y="410" width="560" height="40" class="even-row"/>
  <text x="35" y="435" class="text">4</text>
  <text x="80" y="435" class="text">David Lee</text>
  <text x="220" y="435" class="text">david@example.com</text>
  <text x="360" y="435" class="text">User</text>
  <rect x="450" y="420" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="460" y="435" class="button-text">Voir</text>
  <rect x="505" y="420" width="50" height="25" rx="4" fill="#5bc0de"/>
  <text x="515" y="435" class="button-text">Éditer</text>
  
  <!-- Browser Frame -->
  <rect x="5" y="5" width="590" height="490" stroke="#ccc" stroke-width="2" fill-opacity="0" rx="5"/>
  <rect x="5" y="5" width="590" height="25" fill="#f0f0f0" rx="5" ry="0"/>
  <text x="20" y="22" font-family="Arial" font-size="12" fill="#333">http://localhost:3000/</text>
</svg>
