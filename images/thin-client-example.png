<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <style>
    .title { font-family: Arial; font-size: 24px; font-weight: bold; }
    .heading { font-family: Arial; font-size: 18px; font-weight: bold; }
    .table { fill: #f5f5f5; stroke: #333; stroke-width: 1; }
    .header { fill: #e1e1e1; }
    .odd-row { fill: #ffffff; }
    .even-row { fill: #f9f9f9; }
    .text { font-family: Arial; font-size: 14px; }
  </style>
  
  <rect width="600" height="400" fill="white"/>
  
  <text x="20" y="40" class="title">Liste d'utilisateurs (Client Léger)</text>
  <text x="20" y="70" class="text">Cette page est entièrement générée par le serveur</text>
  
  <!-- Table -->
  <rect x="20" y="90" width="560" height="200" class="table" rx="4"/>
  
  <!-- Table Header -->
  <rect x="20" y="90" width="560" height="30" class="header" rx="4"/>
  <text x="40" y="110" class="heading">ID</text>
  <text x="100" y="110" class="heading">Nom</text>
  <text x="300" y="110" class="heading">Email</text>
  <text x="450" y="110" class="heading">Rôle</text>
  <text x="520" y="110" class="heading">Actions</text>
  
  <!-- Table Rows -->
  <rect x="20" y="120" width="560" height="40" class="odd-row"/>
  <text x="40" y="145" class="text">1</text>
  <text x="100" y="145" class="text">Alice Smith</text>
  <text x="300" y="145" class="text">alice@example.com</text>
  <text x="450" y="145" class="text">Admin</text>
  <rect x="520" y="130" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="530" y="145" class="text" fill="white">Voir</text>
  
  <rect x="20" y="160" width="560" height="40" class="even-row"/>
  <text x="40" y="185" class="text">2</text>
  <text x="100" y="185" class="text">Bob Johnson</text>
  <text x="300" y="185" class="text">bob@example.com</text>
  <text x="450" y="185" class="text">User</text>
  <rect x="520" y="170" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="530" y="185" class="text" fill="white">Voir</text>
  
  <rect x="20" y="200" width="560" height="40" class="odd-row"/>
  <text x="40" y="225" class="text">3</text>
  <text x="100" y="225" class="text">Charlie Brown</text>
  <text x="300" y="225" class="text">charlie@example.com</text>
  <text x="450" y="225" class="text">User</text>
  <rect x="520" y="210" width="50" height="25" rx="4" fill="#0275d8"/>
  <text x="530" y="225" class="text" fill="white">Voir</text>
  
  <!-- Navigation -->
  <text x="20" y="280" class="text">
    <tspan fill="#0275d8">Retour à l'accueil</tspan>
  </text>
  
  <!-- Browser Frame -->
  <rect x="5" y="5" width="590" height="390" stroke="#ccc" stroke-width="2" fill-opacity="0" rx="5"/>
  <rect x="5" y="5" width="590" height="25" fill="#f0f0f0" rx="5" ry="0"/>
  <text x="20" y="22" font-family="Arial" font-size="12" fill="#333">http://localhost:3000/thin</text>
</svg>
